WEIXIN_MINIAPP_SECRET=

# 微信支付设置
WEIXIN_PAYMENT_APPID= # 商家小程序或公众号 appid
WEIXIN_PAYMENT_MCHID= # 商家子账号 mchid
WEIXIN_PAYMENT_ISV_APPID= # 服务商 APPID
WEIXIN_PAYMENT_ISV_MCHID=
WEIXIN_PAYMENT_ISV_KEY=
