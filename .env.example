WEIXIN_OFFICIAL_KEY= #APP ID
WEIXIN_OFFICIAL_SECRET=
WEIXIN_OFFICIAL_TOKEN=
WEIXIN_OFFICIAL_AES_KEY=
WEIXIN_OFFICIAL_REDIRECT_URI="" # 授权回调地址

# 微信小程序设置
